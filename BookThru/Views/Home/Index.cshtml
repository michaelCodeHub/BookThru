@{
    ViewData["Title"] = "Home Page";
}

<section>
    <div class="block remove-top">
        <div class="container fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="main-slider overlape">
                        <div id="full-slider-wrapper">
                            <div id="layerslider" style="width:100%;height:820px; ">

                                <div class="ls-slide" data-ls="transition3d:all; timeshift:-1000; slidedelay: 7000;">

                                    <img src="http://placehold.it/1920x800" class="ls-bg" alt="Slide background" />

                                    <div class="ls-slide text-color" style="font-family:Mr Dafoe; font-size:36px; letter-spacing:0px; top:240px; left:20px;" data-ls="easingin:easeOutBack; delayin:1000; offsetxin:top; scalexin:1.3; durationin:2000; parallaxlevel:1;">Popular Book</div>

                                    <div class="ls-slide" style="font-family:Rubik; font-size:60px; font-weight:500; color:#202020; letter-spacing:0px; top:295px; left:0;" data-ls="easingin:easeOutBack; delayin:1000; offsetxin:top; scalexin:1.3; durationin:2000; parallaxlevel:1;">Bidding ends in <strong class="text-color" style="font-weight: 600;">2 Days</strong></div>

                                    <div class="ls-slide" style="font-family:Rubik; line-height: 24px; font-size:16px; font-weight:normal; color:#858585; letter-spacing:0px; top:375px; left:0;" data-ls="easingin:easeOutBack; delayin:1000; offsetxin:top; scalexin:1.3; durationin:2000; parallaxlevel:1;">abcd.</div>

                                    <a href="#" title="" class="ls-slide bg-color" style="padding:14px 30px; text-transform: uppercase; color:#ffffff; font-family:Rubik; font-size:14px; font-weight: 500; top:445px; left:0;" data-ls="easingin:easeOutBack; delayin:2000; offsetxin:bottom; rotatexin:90; durationin:2000; parallaxlevel:1.5;">bid now</a>
                                </div>
                                <!-- Slide 2 -->

                                <div class="ls-slide" data-ls="transition3d:all; timeshift:-1000; slidedelay: 7000;">

                                    <img src="http://placehold.it/1920x800" class="ls-bg" alt="Slide background" />

                                    <div class="ls-slide text-color" style="font-family:Mr Dafoe; font-size:36px; letter-spacing:0px; top:240px; left:570px;" data-ls="easingin:easeOutBack; delayin:1000; offsetxin:top; scalexin:1.3; durationin:2000; parallaxlevel:1;">New Book</div>

                                    <div class="ls-slide" style="font-family:Rubik; font-size:60px; font-weight:500; color:#202020; letter-spacing:0px; top:295px; left:550px;" data-ls="easingin:easeOutBack; delayin:1000; offsetxin:top; scalexin:1.3; durationin:2000; parallaxlevel:1;">Bidding ends in <strong class="text-color" style="font-weight: 600;">2 hours</strong></div>

                                    <div class="ls-slide" style="font-family:Rubik; line-height: 24px; font-size:16px; font-weight:normal; color:#858585; letter-spacing:0px; top:375px; left:550px;" data-ls="easingin:easeOutBack; delayin:1000; offsetxin:top; scalexin:1.3; durationin:2000; parallaxlevel:1;">abcd.</div>

                                    <a href="#" title="" class="ls-slide bg-color" style="padding:14px 30px; text-transform: uppercase; color:#ffffff; font-family:Rubik; font-size:14px; font-weight: 500; top:445px; left:550px;" data-ls="easingin:easeOutBack; delayin:2000; offsetxin:bottom; rotatexin:90; durationin:2000; parallaxlevel:1.5;">bid now</a>
                                </div>
                            </div>
                        </div>
                    </div><!-- Main Slider -->
                </div>
            </div>
        </div>
    </div>
</section>


