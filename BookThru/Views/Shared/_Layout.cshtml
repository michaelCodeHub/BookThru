        <partial name="_CookieConsentPartial" />

        <div class="">
            @RenderBody()
        </div>

        <section>
            <div class="block gray less-top less-bottom">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="subscribetitle">
                                <h3>Checkout our new books</h3>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </section>

        <!-- Footer -->
        <footer>
            <div class="block">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-3 column">
                            <div class="widget">
                                <div class="link-widget">
                                    <h3>QUESTIONS</h3>
                                    <a href="#" title="">How to Buy</a>
                                    <a href="#" title="">Payment</a>
                                    <a href="#" title="">Shipping</a>
                                    <a href="#" title="">Other FAQs</a>
                                </div>
                            </div><!-- Widget  -->
                        </div>
                        <div class="col-lg-3 column">
                            <div class="widget">
                                <div class="link-widget">
                                    <h3>CATEGORIES</h3>
                                    <a href="#" title="">Academic</a>
                                    <a href="#" title="">Fiction</a>
                                    <a href="#" title="">Others</a>

                                </div>
                            </div><!-- Widget  -->
                        </div>

                        <div class="col-lg-3 column">
                            <div class="widget">
                                <div class="contact-widget">
                                    <h3>Contact Us</h3>
                                    <span><i class="la la-clock-o"></i>Monday - Sunday: 09:00 - 21:00</span>
                                    <span><i class="la la-map-marker"></i>A Block</span>
                                    <span><i class="la la-phone"></i> 416 - 123 - 4567 </span>
                                    <span><i class="la la-envelope"></i>info@bookthru.com</span>
                                </div>
                            </div><!-- Widget  -->
                        </div>
                    </div>
                </div>
            </div>
            <div class="bottomline">
                <div class="container">
                    <span>© 2019 BookThru. All rights reserved!. Powered by Centennial Students.</span>
                </div>
            </div>
        </footer>
    </div>

    <div class="searchdialouge">
        <div class="searchpopup">
            <h3>Search</h3><span><i class="ti-close"></i></span>
            <form>
                <input type="text" placeholder="Search here" />
                <button type="submit"><i class="la la-search"></i></button>
            </form>
        </div>
    </div><!-- Search  -->


    <script src="~/js/jquery.min.js" type="text/javascript"></script>
    <script src="~/js/modernizr.js" type="text/javascript"></script>
    <script src="~/js/script.js" type="text/javascript"></script>
    <script src="~/js/wow.min.js" type="text/javascript"></script>
    <script src="~/js/isotop.js" type="text/javascript"></script>
    <script src="~/js/slick.min.js" type="text/javascript"></script>
    <script src="~/js/value.js" type="text/javascript"></script>

    <script src="~/js/slick.min.js" type="text/javascript"></script>
    <!-- External libraries: jQuery & GreenSock -->
    <script src="~/layerslider/js/greensock.js" type="text/javascript"></script>
    <!-- LayerSlider script files -->
    <script src="~/layerslider/js/layerslider.transitions.js" type="text/javascript"></script>
    <script src="~/layerslider/js/layerslider.kreaturamedia.jquery.js" type="text/javascript"></script>

    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.3.1.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
